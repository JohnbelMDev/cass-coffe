<!doctype html>
<html>

<head>
  <title>Cafe Cass</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="style.css">
</head>

<body>
  <div class="container">

    <div class="page-header text-center">
      <h1><span class="fa fa-anchor"></span> Admin Page</h1>
      <a href="/profile" class="btn btn-default btn-sm">Check order Queue</a>
      <a href="/logout" class="btn btn-default btn-sm">Logout</a>
    </div>




  </div>

  <div class="container">
    <ul class="">
      <% for(var i=0; i<completed.length; i++) {%>
      <li class="">
        <span>Order completed : <%= completed[i].order %></span><br>
        <span>Customer Name: <%= completed[i].name %></span><br>
        <label for="barista"><%= completed[i].barista %></label>
        <span><i class="fa fa-trash" aria-hidden="true"></i></span>
      </li>
      <% } %>
    </ul>
  </div>



  <script src="order.js"></script>

</body>

</html>